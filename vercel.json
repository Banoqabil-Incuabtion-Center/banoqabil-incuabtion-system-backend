{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron-handler",
      "schedule": "30 19 * * *"
    }
  ],
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cron-handler",
      "dest": "api/cron-handler.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}